# Render Configuration
PORT=3000
NODE_ENV=production

# Redis Cache
REDIS_URL=redis://localhost:6379

# SOCKS5 Proxies (Recommended)
SOCKS5_PROXY_1=socks5://username:password@proxy1.example.com:1080
SOCKS5_PROXY_2=socks5://username:password@proxy2.example.com:1080

# HTTPS Proxies
HTTPS_PROXY_1=https://username:password@proxy3.example.com:3128
HTTPS_PROXY_2=https://username:password@proxy4.example.com:3128

# Residential Proxies (for undetectability)
RESIDENTIAL_PROXY_1=socks5://user:pass@residential-proxy.example.com:1080

# Security
ALLOWED_DOMAINS=*
RATE_LIMIT_PER_MINUTE=100
